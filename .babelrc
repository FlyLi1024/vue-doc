{
  "presets": [
    [
      "@babel/preset-env", // 将ES6语法转换为es5
      {
        "useBuiltIns": "usage", // 只编译需要编译的代码
        "corejs": "3.0.1"
      }
    ]
  ],
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "less",
          "css",
          "html",
          "md"
        ],
        "plugins": [
          "inline-color",
          "copy-to-clipboard",
          "previewers",
          "autolinker",
          "line-highlight"
        ],
        "theme": "default",
        "css": true
      }
    ]
    // [
    //   "component",
    //   {
    //     "libraryName": "@kad/design",
    //     "styleLibraryName": "theme-chalk"
    //   }
    // ]
  ]
}